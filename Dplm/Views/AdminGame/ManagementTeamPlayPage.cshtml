
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Управление играющими командами</title>
</head>
<body>
    <div>
        <table border="1">
            <tr>
                <th>Команда</th>
                <th>Участие</th>
                <th>Действие</th>
            </tr>
            @{
                foreach (var tableRow in ViewBag.TableRows)
                {
                    @Html.Raw(tableRow)
                }
            }
        </table>
    </div>
</body>
</html>
