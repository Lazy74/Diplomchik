<script src="/Scripts/Command/CommandPage.js"></script>
<div>


    <div class="add-player">
        <input type="text" id="player-name" placeholder="Логин игрока" data-bind="value: addNewPlayer" />
        <button id="btn-add-player" data-bind="click: addPlayer">Добавить игрока</button>
    </div>

    <span>Название команды: @ViewBag.teamName</span>
    <br />
    <span>@ViewBag.commander</span>
    <br />
    <span>id команды: @ViewBag.teamId</span>

    <table>
        @{
            foreach (var name in ViewBag.playerName)
            {
                <tr><td>@name</td></tr>
            }
        }
    </table>
</div>

<div>
    <button data-bind="click: outTeam">Выйти из команды</button>
</div>

<script>
    loadPage();
</script>