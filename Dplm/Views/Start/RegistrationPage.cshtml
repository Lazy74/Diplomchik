
@{
    ViewBag.Title = "RegistrationPage";
}

<html>
<head>
    <link href="/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="/Content/Site.css" rel="stylesheet" />

    <script src="/Scripts/jquery-1.10.2.min.js"></script>
    <script src="/Scripts/knockout-3.4.0.js"></script>
    <script src="/Scripts/Registration.js"></script>
</head>
<body>
<div>
    <input type="text" id="user-login" class="form-control" placeholder="Логин" data-bind="value: userLogin"/>
    <input type="text" id="user-pass" class="form-control" placeholder="Пароль" data-bind="value: userPass"/>
    <input type="text" id="repet-user-pass" class="form-control" placeholder="Повт. пароль" data-bind="value: confirmUserPass"/>
    <input type="text" id="phone-number" class="form-control" placeholder="Номер телефона" data-bind="value: phoneNumber"/>
    <input type="text" id="email" class="form-control" placeholder="Эл. почта" data-bind="value: email"/>
    <button id="btn-create" class="btn btn-success" data-bind="click: createUser">Зарегистрироваться</button>
</div>
<script>
    loadPage();
</script>
</body>
</html>

