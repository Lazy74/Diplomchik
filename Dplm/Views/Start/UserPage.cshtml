<script src="/Scripts/User/UserPage.js"></script>
<div class="userPage">
    <div>
        <span>Имя: @ViewBag.firstName</span>
    </div>
    <div>
        <span>Фамилия: @ViewBag.lastName</span>
    </div>
    <div>
        <span>Номер телефона: @ViewBag.phoneNumber</span>
    </div>
    <div>
        <span>Электронная почта: @ViewBag.email</span>
    </div>
    <div>
        <span>Дата рождения: @ViewBag.birthday</span>
    </div>
    <div>
        <span>Ссылка ВКонтакте: @ViewBag.linkVK</span>
    </div>
    <button id="btn-update" class="btn btn-success btn-block" data-bind="click: updateUser">Изменить данные</button>
</div>

<script>
    loadPage();
</script>